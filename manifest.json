{
    "name": "facebookParser",
    "version": "1.0",
  "manifest_version": 2,

  "description": "Build an Extension!",
     "icons": {
       "16": "images/scrather16.png",
       "32": "images/scrather32.png",
       "64": "images/scrather64.png",
       "128": "images/scrather128.png"
     },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],
  "browser_action": {
    "default_title": "Test Extension"
  },

  "permissions": ["activeTab"]
}
